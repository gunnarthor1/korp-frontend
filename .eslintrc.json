{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": "standard",
  "plugins": [],
  "rules": {
    "comma-spacing": "off",
    "quotes": "off",
    "key-spacing": "off",
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": "off",
    "indent": "off",
    "space-before-function-paren": "off",
    "no-trailing-spaces": "off",
    "padded-blocks": "off",
    "space-in-parens": "off",
    "comma-dangle": "off",
    "no-multi-spaces": "off",
    "keyword-spacing": "off",
    "eqeqeq": "off",
    "camelcase": "off"
  },
  "globals": {
    "angular": true,
    "korpApp": true,
    "settings": true,
    "c": true,
    "_": true,
    "$": true,
    "corpusChooserInstance": true,
    "currentMode": true,
    "isLab": true,
    "getAllCorporaInFolders": true,
    "authenticationProxy": true,
    "util": true,
    "safeApply": true,
    "CQP": true,
    "moment": true,
    "regescape": true,
    "unregescape": true,
    "model": true,
    "kwicResults": true,
    "lemgramResults": true,
    "statsResults": true,
    "view": true,
    "timeProxy": true,
    "CSV": true
  }
}
